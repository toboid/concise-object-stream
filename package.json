{
  "name": "concise-object-stream",
  "version": "1.0.5",
  "description": "More concise node object streams",
  "main": "dist/index.js",
  "scripts": {
    "precompile": "rm -f dist/*",
    "compile": "babel lib --out-dir dist",
    "coverage": "nyc --require babel-core/register --reporter=lcov mocha ./test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint 'lib/**' 'test/**'",
    "test": "nyc --check-coverage --require babel-core/register mocha ./test",
    "test-dist": "TEST_TARGET=dist mocha --require babel-core/register",
    "watch": "chokidar 'lib/*.js' 'test/*.js' -c 'npm run lint && npm test'",
    "ci": "npm run lint && npm run compile && npm run test-dist && npm run coverage",
    "prepublish": "npm run compile"
  },
  "keywords": [
    "streams"
  ],
  "author": "toboid",
  "license": "MIT",
  "respository": "toboid/concise-object-stream",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "concat-stream": "^1.5.1",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.2",
    "eslint-config-toboid": "^1.0.1",
    "lodash": "^4.8.2",
    "mocha": "^2.4.5",
    "nyc": "^6.4.0"
  },
  "dependencies": {
    "promise": "^7.1.1",
    "readable-stream": "^2.0.6"
  }
}
