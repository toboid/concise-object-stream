    "chokidar-cli": "^1.2.2",
    "concat-stream": "^2.0.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-toboid": "2.0.0",
    "lodash": "^4.17.11",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0"
  },
  "dependencies": {
    "readable-stream": "^3.3.0"
  }
}
